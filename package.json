{
  "name": "fastify-zod-query-coercion",
  "version": "0.2.2",
  "description": "A Fastify plugin that automatically converts query parameter types based on Zod schemas",
  "keywords": [
    "fastify",
    "zod",
    "query",
    "coercion",
    "parameter",
    "validation",
    "plugin",
    "schema",
    "parser",
    "type-safe",
    "querystring",
    "preprocessing",
    "fastify-plugin",
    "zod-validation",
    "url-params",
    "typescript",
    "middleware",
    "request-validation"
  ],
  "homepage": "https://github.com/jsnimda/fastify-zod-query-coercion#readme",
  "bugs": {
    "url": "https://github.com/jsnimda/fastify-zod-query-coercion/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsnimda/fastify-zod-query-coercion.git"
  },
  "license": "MIT",
  "author": "",
  "type": "module",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.ts"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "crackle package",
    "crackle:fix": "crackle fix",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage && vite preview --outDir coverage --open",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@fastify/error": "^4.0.0",
    "fastify-plugin": "^5.0.1"
  },
  "devDependencies": {
    "@crackle/cli": "^0.15.5",
    "@vitest/coverage-v8": "^2.1.1",
    "@vitest/ui": "^2.1.1",
    "fastify": "^5.0.0",
    "vitest": "^2.1.1",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.4.0"
  },
  "peerDependencies": {
    "fastify": "^4.0.0 || ^5.0.0",
    "zod": "^3.21.4"
  }
}
